{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "impliedStrict": true
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [ "error", 4 ],
        "linebreak-style": [ "error", "unix" ],
        "semi": [ "error", "always" ],
        "comma-dangle": [ "error", "always-multiline" ],
        "no-cond-assign": [ "error", "always" ],
        "no-console": "off",
        "no-unused-vars": [ "warn", { "vars": "local", "args": "none" } ]
    }
}
